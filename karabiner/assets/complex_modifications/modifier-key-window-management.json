{
  "title": "Mod Key Window Management",
  "rules": [
    {
      "description": "fn + Left ⌃ | Maximize Window Left",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_control",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "osascript -e 'tell application \"BetterTouchTool\" to execute_assigned_actions_for_trigger \"14615871-78D8-49A7-A95E-F292366825EA\"'"
            }
          ]
        }
      ]
    },
    {
